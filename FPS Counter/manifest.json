{
	"$schema": "https://raw.githubusercontent.com/beat-saber-modding-group/BSIPA-MetadataFileSchema/master/Schema.json",
	"id": "FPSCounter",
	"name": "FPS Counter",
	"icon": "FPS_Counter.Resources.Icon.png",
	"description": "A Beat Saber mod to show your fps in-game.",
	"author": "Eris",
	"gameVersion": "1.16.4",
	"version": "3.0.9",
	"dependsOn": {
		"BSIPA": "^4.2.0",
		"BeatSaberMarkupLanguage": "^1.5.5",
		"SiraUtil": "^2.5.8"
	},
	"loadAfter": [
		"Counters+"
	],
	"features": {
		"CountersPlus.CustomCounter": {
			"Name": "FPS Counter",
			"CounterLocation": "FPS_Counter.Counters.FpsCounterCountersPlus",
			"BSML": {
				"Resource": "FPS_Counter.Settings.UI.Views.mainSettingsCountersPlus.bsml",
				"Host": "FPS_Counter.Settings.UI.SettingsController",
				"Icon": "FPS_Counter.Resources.Icon.png"
			}
		}
	},
	"links": {
		"project-source": "https://github.com/ErisApps/FPS-Counter"
	}
}